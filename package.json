  "description": "",
  "dependencies": {
    "express": "^4.21.0",
    "ts-node": "^10.9.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^22.5.5",
    "concurrently": "^9.0.1"
  }
}
